{
  "name": "popcorn4dinner/presenters",
  "description": "A non-opinionated implementation of the presenter pattern",
  "type": "library",
  "version": "0.1.2",
  "license": "MIT",
  "config": {
    "preferred-install": "dist"
  },
  "repositories": {
    "packagist.org": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  },
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7"
  },
  "autoload": {
    "psr-4": {
      "Popcorn4dinner\\Presenters\\": "src/"
    }
  },
    "autoload-dev": {
      "psr-4": {
        "Popcorn4dinner\\Presenters\\Examples\\": "examples/",
        "Popcorn4dinner\\Presenters\\Tests\\": "tests/unit/"
      }
    }
}
